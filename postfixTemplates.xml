<application>
  <component name="PostfixTemplates">
    <template id="ifnull@userDefined" key=".ifn" provider="builtin.java" language-level="1.3" topmost="false">
      <conditions>
        <condition id="notPrimitive" />
      </conditions>
      <template name="fakeKey" value="if(Objects.isNull($EXPR$)){$END$}" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="ifnotnull@userDefined" key=".ifnn" provider="builtin.java" language-level="1.3" topmost="false">
      <conditions>
        <condition id="notPrimitive" />
      </conditions>
      <template name="fakeKey" value="if(Objects.nonNull($EXPR$)){$END$}" toReformat="true" toShortenFQNames="true" />
    </template>
  </component>
</application>